<?xml version="1.0" encoding="UTF-8"?>
<!-- Published by JAX-WS RI at http://jax-ws.dev.java.net. RI's version is Oracle JAX-WS 2.1.5. -->
<xs:schema xmlns:tns="http://ejb.coreapps.apps.trellis.avocent.com/" xmlns:ns1="http://xmlns.avocent.com/commonplatform/objectstatechangednotification" xmlns:xs="http://www.w3.org/2001/XMLSchema" version="1.0" targetNamespace="http://ejb.coreapps.apps.trellis.avocent.com/">

<xs:import namespace="http://xmlns.avocent.com/commonplatform/objectstatechangednotification" schemaLocation="ObjectStateChangedNotificationType.xsd"/>

<xs:element name="getContainerState" type="tns:getContainerState"/>

<xs:element name="getContainerStateResponse" type="tns:getContainerStateResponse"/>

<xs:element name="getParentContainerState" type="tns:getParentContainerState"/>

<xs:complexType name="getContainerState">
<xs:sequence>
<xs:element name="arg0" type="xs:string" minOccurs="0" maxOccurs="unbounded"/>
</xs:sequence>
</xs:complexType>

<xs:complexType name="getContainerStateResponse">
<xs:sequence>
<xs:element name="return" type="tns:objectStateInformationDto" minOccurs="0" maxOccurs="unbounded"/>
</xs:sequence>
</xs:complexType>

<xs:complexType name="objectStateInformationDto">
<xs:sequence>
<xs:element name="alarmSeverity" type="tns:alarmStatus" minOccurs="0"/>
<xs:element name="objectId" type="xs:string" minOccurs="0"/>
<xs:element name="objectType" type="xs:string" minOccurs="0"/>
<xs:element name="state" type="tns:deviceState" minOccurs="0"/>
</xs:sequence>
</xs:complexType>

<xs:complexType name="getParentContainerState">
<xs:sequence>
<xs:element name="arg0" type="ns1:ObjectStateChangedNotificationType" minOccurs="0"/>
</xs:sequence>
</xs:complexType>

<xs:simpleType name="alarmStatus">
<xs:restriction base="xs:string">
<xs:enumeration value="NOALARM"/>
<xs:enumeration value="INFORMATIONAL"/>
<xs:enumeration value="WARNING"/>
<xs:enumeration value="CRITICAL"/>
</xs:restriction>
</xs:simpleType>

<xs:simpleType name="deviceState">
<xs:restriction base="xs:string">
<xs:enumeration value="DEVICE_STATE_NORMAL"/>
<xs:enumeration value="DEVICE_STATE_IN_MAINTENANCE_MODE"/>
<xs:enumeration value="DEVICE_STATE_ALARM"/>
<xs:enumeration value="DEVICE_STATE_NOT_RESPONDING"/>
</xs:restriction>
</xs:simpleType>
</xs:schema>
