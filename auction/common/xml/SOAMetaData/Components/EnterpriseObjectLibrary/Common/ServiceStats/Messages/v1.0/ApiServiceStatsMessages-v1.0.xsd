<?xml version='1.0' encoding='UTF-8'?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns="http://xmlns.avocent.com/Common/ServiceStatistics/ApiServiceStats/Messages/v1.0"
	targetNamespace="http://xmlns.avocent.com/Common/ServiceStatistics/ApiServiceStats/Messages/v1.0"
	elementFormDefault="qualified" version="1.0">
	<xsd:element name="Vzid" type="xsd:string" />
	<xsd:element name="AllMonitoredRequest">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Vzid" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="AllMonitoredResponse">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ServiceStatistics" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="SingleMonitoredRequest">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Vzid" />
				<xsd:element name="ServiceName" type="xsd:string" />
				<xsd:element name="ServiceDesc">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="ProxyService" />
							<xsd:enumeration value="BusinessService" />
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ServiceStatistics">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="ServiceRefCollection">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element ref="ServiceRef" maxOccurs="unbounded" />
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ServiceRef">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="ResourceCollection" />
			</xsd:sequence>
			<xsd:attribute name="name">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string" />
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="ResourceCollection">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="Resource" maxOccurs="unbounded" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="Resource">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="StatisticCol">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="Statistic" maxOccurs="unbounded">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="CountValue" type="xsd:string" />
										<xsd:element name="InitialStatus" type="xsd:string" />
										<xsd:element name="CurrentStatus" type="xsd:string" />
										<xsd:element name="MinValue" type="xsd:string" />
										<xsd:element name="MaxValue" type="xsd:string" />
										<xsd:element name="SumValue" type="xsd:string" />
										<xsd:element name="AvgValue" type="xsd:string" />
									</xsd:sequence>
									<xsd:attribute name="name">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string" />
										</xsd:simpleType>
									</xsd:attribute>
									<xsd:attribute name="type">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:enumeration value="COUNT" />
												<xsd:enumeration value="STATUS" />
												<xsd:enumeration value="INTERVAL" />
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:attribute>
								</xsd:complexType>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
			<xsd:attribute name="name">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string" />
				</xsd:simpleType>
			</xsd:attribute>
			<xsd:attribute name="type">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string" />
				</xsd:simpleType>
			</xsd:attribute>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
