<?xml version="1.0" encoding="UTF-8"?>
<!-- Published by JAX-WS RI at http://jax-ws.dev.java.net. RI's version is 
	Oracle JAX-WS 2.1.5. -->
<xs:schema
	xmlns:tns="http://xmlns.avocent.com/commonplatform/cps/utils/reflowutility"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" version="1.0"
	targetNamespace="http://xmlns.avocent.com/commonplatform/cps/utils/reflowutility">

	<xs:element name="reflow" type="tns:reflow" />

	<xs:element name="reflowResponse" type="tns:reflowResponse" />

	<xs:complexType name="reflow">
		<xs:sequence>
			<xs:element name="ReflowParameters" type="tns:ReflowParameter"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="ReflowParameter">
		<xs:sequence>
			<xs:element name="executionProtocol" type="xs:string" />
			<xs:element name="hostName" type="xs:string" />
			<xs:element name="portNumber" type="xs:int" />
			<xs:element name="isOneWayService" type="xs:boolean" />
			<xs:element name="serviceAddressURL" type="xs:string" />
			<xs:element name="wsOperation" type="xs:string" />
			<xs:element name="xmlPayload" type="xs:string" />
			<xs:element name="serviceTargetNamespace" type="xs:string"
				minOccurs="0" />
			<xs:element name="userName" type="xs:string" minOccurs="0" />
			<xs:element name="password" type="xs:string" minOccurs="0" />
			<xs:element name="attachment" type="tns:Attachment"
				minOccurs="0" />
			<xs:element name="headerList" type="tns:Header" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Attachment">
		<xs:sequence>
			<xs:element name="contentId" type="xs:string" minOccurs="0" />
			<xs:element name="contentType" type="xs:string" />
			<xs:element name="content" type="xs:base64Binary" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Header">
		<xs:sequence>
			<xs:element name="namespace" type="xs:string" minOccurs="0" />
			<xs:element name="name" type="xs:string" />
			<xs:element name="value" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="reflowResponse">
		<xs:sequence>
			<xs:element name="Status" type="xs:boolean" />
		</xs:sequence>
	</xs:complexType>
</xs:schema>