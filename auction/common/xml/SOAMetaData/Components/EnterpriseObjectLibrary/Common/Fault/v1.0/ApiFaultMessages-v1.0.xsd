<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://xmlns.avocent.com/Common/Fault/ApiFault/Messages/v1.0" targetNamespace="http://xmlns.avocent.com/Common/Fault/ApiFault/Messages/v1.0" elementFormDefault="qualified">
    <xsd:element name="ReportFault" type="ReportFaultType"/>
    <xsd:element name="CorrelationID" type="xsd:string"/>
    <xsd:element name="ServiceName" type="xsd:string"/>
    <xsd:element name="ErrorMessage" type="xsd:string"/>
    <xsd:element name="FaultTime" type="xsd:dateTime"/>
    <xsd:element name="LocalPsProtocol" type="xsd:string"/>
    <xsd:element name="OperationName" type="xsd:string"/>
    <xsd:complexType name="ReportFaultType">
        <xsd:sequence>
            <xsd:element ref="CorrelationID"/>
            <xsd:element ref="ServiceName"/>
            <xsd:element name="FaultDescription" type="xsd:string"/>
            <xsd:element name="FaultCategory" type="xsd:string"/>
            <xsd:element name="FaultLocation" type="xsd:string"/>
            <xsd:element ref="FaultTime"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="ReportCommunicationFault" type="ReportCommunicationFaultType"/>
    <xsd:complexType name="ReportCommunicationFaultType">
        <xsd:sequence>
            <xsd:element ref="CorrelationID"/>
            <xsd:element ref="ServiceName"/>
            <xsd:element name="FaultDescription" type="xsd:string"/>
            <xsd:element name="Destination" type="xsd:string"/>
            <xsd:element ref="FaultTime"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="ReportCommunicationFaultWithReflow" type="ReportCommunicationFaultWithReflowType"/>
    <xsd:complexType name="ReportCommunicationFaultWithReflowType">
        <xsd:sequence>
            <xsd:element ref="CorrelationID"/>
            <xsd:element ref="ServiceName"/>
            <xsd:element name="FaultDescription" type="xsd:string"/>
            <xsd:element name="ReflowUrl" type="xsd:string"/>
            <xsd:element name="Destination" type="xsd:string"/>
            <xsd:element ref="FaultTime"/>
            <xsd:element ref="LocalPsProtocol"/>
            <xsd:element ref="OperationName"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="ReportDisabledFault" type="DisabledFaultType"/>
    <xsd:complexType name="DisabledFaultType">
        <xsd:sequence>
            <xsd:element ref="CorrelationID"/>
            <xsd:element ref="ServiceName"/>
            <xsd:element name="ReflowUrl" type="xsd:string"/>
            <xsd:element name="Destination" type="xsd:string"/>
            <xsd:element ref="FaultTime"/>
            <xsd:element ref="LocalPsProtocol"/>
            <xsd:element ref="OperationName"/>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>