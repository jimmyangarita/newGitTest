<?xml version='1.0' encoding='UTF-8'?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns="http://xmlns.avocent.com/Common/Audit/ApiAudit/Messages/v1.0"
            targetNamespace="http://xmlns.avocent.com/Common/Audit/ApiAudit/Messages/v1.0"
            elementFormDefault="qualified" version="1.0">
    <xsd:element name="AuditInput" type="AuditInputType"/>
    <xsd:element name="AuditOutput" type="AuditOutputType"/>
    <xsd:element name="AuditResponse" type="AuditResponseType"/>
    <xsd:element name="AuditCheckPoint" type="AuditCheckPointType"/>
    <xsd:element name="CorrelationID" type="xsd:string"/>
    <xsd:element name="ServiceName" type="xsd:string"/>
    <xsd:element name="Payload" type="xsd:anyType"/>
    <xsd:element name="CallerTime" type="xsd:dateTime"/>
    <xsd:element name="Code" type="xsd:string"/>
    
    <xsd:element name="InterfaceName" type="xsd:string"/>
    <xsd:element name="ServiceProvider" type="xsd:string"/>
    <xsd:element name="ServiceConsumer" type="xsd:string"/>
    <xsd:element name="RequestID" type="xsd:string"/>
    <xsd:element name="TransactionID" type="xsd:string"/>
    <xsd:element name="BusinessProcessID" type="xsd:string"/>
    <xsd:element name="UserID" type="xsd:string"/>
    <xsd:element name="SenderID" type="xsd:string"/>
    <xsd:element name="ReferenceID" type="xsd:string"/>
    <xsd:element name="SenderName" type="xsd:string"/>
    <xsd:element name="SenderTZ" type="xsd:string"/>
    
    <xsd:complexType name="AuditOutputType">
        <xsd:sequence>
            <xsd:element ref="CorrelationID"/>
            <xsd:element ref="ServiceName"/>
            <xsd:element ref="CallerTime"/>
            <xsd:element ref="Code"/>
            <xsd:element name="Destination" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="AuditResponseType">
        <xsd:sequence>
            <xsd:element ref="CorrelationID"/>
            <xsd:element ref="ServiceName"/>
            <xsd:element ref="CallerTime"/>
            <xsd:element ref="Code"/>
            <xsd:element name="ResponseType" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="AuditCheckPointType">
        <xsd:sequence>
            <xsd:element ref="CorrelationID"/>
            <xsd:element ref="ServiceName"/>
            <xsd:element ref="CallerTime"/>
            <xsd:element ref="Code"/>
            <xsd:element name="Location" type="xsd:string"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="AuditInputType">
        <xsd:sequence>
            <xsd:element ref="CorrelationID"/>
            <xsd:element ref="ServiceName"/>
            <xsd:element ref="Payload"/>
            <xsd:element ref="CallerTime"/>
            <xsd:element ref="Code"/>
            <xsd:element name="Origination" type="xsd:string"/>
            <xsd:element ref="InterfaceName"/>
            <xsd:element ref="ServiceProvider"/>
            <xsd:element ref="ServiceConsumer"/>
            <xsd:element ref="RequestID"/>
            <xsd:element ref="TransactionID"/>
            <xsd:element ref="BusinessProcessID"/>
            <xsd:element ref="UserID"/>
            <xsd:element ref="SenderID"/>
            <xsd:element ref="ReferenceID"/>
            <xsd:element ref="SenderName"/>
            <xsd:element ref="SenderTZ"/>
        </xsd:sequence>
    </xsd:complexType>
    
</xsd:schema>
