<?xml version="1.0" encoding="UTF-8"?>
<xs:schema
	xmlns:tns="http://facade.alarms.ejb.services.cps.commonplatform.avocent.com/"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" version="1.0"
	targetNamespace="http://facade.alarms.ejb.services.cps.commonplatform.avocent.com/">

	<xs:element name="CdmException" type="tns:CdmException" />

	<xs:element name="MissingDataException" type="tns:MissingDataException" />

	<xs:element name="evaluateStateChange" type="tns:evaluateStateChange" />

	<xs:element name="evaluateStateChangeResponse" type="tns:evaluateStateChangeResponse" />

	<xs:complexType name="evaluateStateChange">
		<xs:sequence>
			<xs:element name="arg0" type="xs:string" minOccurs="0" />
			<xs:element name="arg1" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="evaluateStateChangeResponse">
		<xs:sequence>
			<xs:element name="return" type="tns:deviceAlarmState"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="deviceAlarmState">
		<xs:sequence>
			<xs:element name="alarmSeverityChanged" type="xs:boolean" />
			<xs:element name="deviceStateChanged" type="xs:boolean" />
			<xs:element name="newAlarmSeverity" type="xs:string"
				minOccurs="0" />
			<xs:element name="newDeviceState" type="xs:string"
				minOccurs="0" />
			<xs:element name="oldAlarmSeverity" type="xs:string"
				minOccurs="0" />
			<xs:element name="oldDeviceState" type="xs:string"
				minOccurs="0" />
			<xs:element name="severeRecentAlarm" type="tns:trellisAlarmTypeEBO"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="trellisAlarmTypeEBO">
		<xs:sequence>
			<xs:element name="acknowledged" type="xs:boolean"
				minOccurs="0" />
			<xs:element name="acknowledgedBy" type="xs:string"
				minOccurs="0" />
			<xs:element name="acknowledgedTimestamp" type="xs:string"
				minOccurs="0" />
			<xs:element name="alarmDescriptionString" type="xs:string"
				minOccurs="0" />
			<xs:element name="alarmDetailDescriptionKey" type="xs:string"
				minOccurs="0" />
			<xs:element name="alarmInstanceId" type="xs:string"
				minOccurs="0" />
			<xs:element name="alarmNameKey" type="xs:string"
				minOccurs="0" />
			<xs:element name="alarmStateKey" type="xs:string"
				minOccurs="0" />
			<xs:element name="alarmTypeTag" type="xs:string"
				minOccurs="0" />
			<xs:element name="componentId" type="xs:string" minOccurs="0" />
			<xs:element name="componentName" type="xs:string"
				minOccurs="0" />
			<xs:element name="createdTimestamp" type="xs:string"
				minOccurs="0" />
			<xs:element name="equipCategory" type="xs:string"
				minOccurs="0" />
			<xs:element name="escalationLevel" type="xs:int" />
			<xs:element name="eventClass" type="xs:string" minOccurs="0" />
			<xs:element name="lastTransitionTimestamp" type="xs:string"
				minOccurs="0" />
			<xs:element name="severityKey" type="xs:string" minOccurs="0" />
			<xs:element name="sourceName" type="xs:string" minOccurs="0" />
			<xs:element name="sourceObjectId" type="xs:string"
				minOccurs="0" />
			<xs:element name="sourceTypeKey" type="xs:string"
				minOccurs="0" />
			<xs:element name="subjectCategoryKey" type="xs:string"
				minOccurs="0" />
			<xs:element name="subjectName" type="xs:string" minOccurs="0" />
			<xs:element name="subjectObjectId" type="xs:string"
				minOccurs="0" />
			<xs:element name="subjectTypeKey" type="xs:string"
				minOccurs="0" />
			<xs:element name="transitionList" type="xs:string"
				minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CdmException">
		<xs:sequence>
			<xs:element name="message" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="MissingDataException">
		<xs:sequence>
			<xs:element name="message" type="xs:string" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
</xs:schema>