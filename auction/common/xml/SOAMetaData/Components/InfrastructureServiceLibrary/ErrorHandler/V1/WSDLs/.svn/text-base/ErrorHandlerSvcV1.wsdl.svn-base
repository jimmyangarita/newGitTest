<definitions
     name="ErrorHandlerSvcV1"
     targetNamespace="http://xmlns.avocent.com/trellis/ErrorHandlerSvc/V1"
     xmlns="http://schemas.xmlsoap.org/wsdl/"
     xmlns:com="http://xmlns.avocent.com/trellis/EnterpriseObjectLibrary/Common/V1"
     xmlns:plnk="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema"
     xmlns:client="http://xmlns.avocent.com/trellis/ErrorHandlerSvc/V1"
    >
    <types>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://xmlns.avocent.com/trellis/EnterpriseObjectLibrary/Common/V1" schemaLocation="../../../../EnterpriseObjectLibrary/Infrastructure/V1/Meta.xsd"/>
        </xsd:schema>
    </types>
    
    <message name="ErrorHandlerSvcRequestMsg">
        <part name="FaultMessage" element="com:Fault"/>
    </message>
    <portType name="ErrorHandlerSvcV1">
        <operation name="initiate">
            <input message="client:ErrorHandlerSvcRequestMsg"/>
        </operation>
    </portType>
</definitions>

