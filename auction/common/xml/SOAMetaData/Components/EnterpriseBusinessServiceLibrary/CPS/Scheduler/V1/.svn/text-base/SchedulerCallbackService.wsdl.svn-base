<?xml version= '1.0' encoding= 'UTF-8' ?>
<wsdl:definitions name="SchedulerCallbackService"
                  targetNamespace="http://cps.avocent.com/Scheduler_jws/SchedulerCallbackService/SchedulerCallbackService"
                  xmlns:ns2="http://xmlns.oracle.com/xdb/AQSCH"
                  xmlns:ns1="http://xmlns.dcim.emerson.com/cps/scheduler/quartz-scheduler/ebm"
                  xmlns:ns4="http://xmlns.oracle.com/xdb/CDMR"
                  xmlns:plnk="http://schemas.xmlsoap.org/ws/2003/05/partner-link/"
                  xmlns:ns3="http://xmlns.avocent.com/commonplatform/event"
                  xmlns:client="http://cps.avocent.com/Scheduler_jws/SchedulerCallbackService/SchedulerCallbackService"
                  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                  xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <wsdl:documentation>SchedulerCallback Web service - This service provides
                        the functionality to update the change in
                        status/progress of the Task/TaskAction. It also raises
                        events for the Success/Failure of Progress/Status
                        update. This service is also responsible for pushing the
                        data pertaining to real-time feedback into the AQ queue.</wsdl:documentation>
    <plnk:partnerLinkType name="SchedulerCallbackService">
        <plnk:role name="SchedulerCallbackServiceProvider">
            <plnk:portType name="client:SchedulerCallbackService"/>
        </plnk:role>
    </plnk:partnerLinkType>
    <wsdl:types>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://xmlns.oracle.com/xdb/CDMR"
                        schemaLocation="../../../../EnterpriseObjectLibrary/CPS/Scheduler/V1/CDMR_PROGRESS_TYPE.xsd"/>
        </xsd:schema>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://xmlns.avocent.com/commonplatform/event"
                        schemaLocation="../../../../EnterpriseObjectLibrary/CPS/Scheduler/V1/TrellisEvent.xsd"/>
        </xsd:schema>
        <xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
            <xsd:import namespace="http://xmlns.dcim.emerson.com/cps/scheduler/quartz-scheduler/ebm"
                        schemaLocation="../../../../EnterpriseObjectLibrary/CPS/Scheduler/V1/CPSQuartzSchedulerEBM.1.0.xsd"/>
        </xsd:schema>
    </wsdl:types>
    <wsdl:message name="SchedulerCallbackServiceRequestMessage">
        <wsdl:part name="payload" element="ns1:ResumeTaskRequestEBM"/>
    </wsdl:message>
    <wsdl:portType name="SchedulerCallbackService">
        <wsdl:operation name="process">
            <wsdl:input message="client:SchedulerCallbackServiceRequestMessage"/>
        </wsdl:operation>
    </wsdl:portType>
</wsdl:definitions>
